{
  "name": "nextjs15-business-starter",
  "description": "Next.js 15 and React19 Website Starter with TypeScript, Tailwind CSS, shadcn, Biome, Husky, Lint-Staged, and more.",
  "version": "1.0.0",
  "private": true,
  "author": "Andi Lilaj",
  "license": "MIT",
  "keywords": ["nextjs", "starter", "typescript, Nextjs15, React19 "],
